{
  "name": "vant-element-notice",
  "version": "0.1.0",
  "description": "A unified dialog API for Vue 2 projects using Element UI or Vant UI",
  "private": false,
  "main": "dist/vant-element-notice.common.js",
  "module": "dist/vant-element-notice.esm.js",
  "unpkg": "dist/vant-element-notice.umd.min.js",
  "jsdelivr": "dist/vant-element-notice.umd.min.js",
  "files": [
    "dist",
    "src/lib"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:lib": "cross-env LIBRARY_BUILD=true vue-cli-service build --target lib --name vant-element-notice src/lib/index.js",
    "prepublishOnly": "npm run build:lib",
    "example": "vue-cli-service serve examples/main.js"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "vue": "^2.6.14"
  },
  "peerDependencies": {
    "element-ui": "^2.15.0",
    "vant": "^2.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.8",
    "cross-env": "^7.0.3",
    "element-ui": "^2.15.14",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vant": "^2.13.6"
  },
  "keywords": [
    "vue",
    "element-ui",
    "vant",
    "dialog",
    "alert",
    "confirm",
    "message"
  ],
  "author": "",
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/vant-element-notice.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/vant-element-notice/issues"
  },
  "homepage": "https://github.com/yourusername/vant-element-notice#readme"
}
